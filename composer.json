{
    "name": "msntx/kit",
    "description": "A sample application for beginning development with Laravel 4.",
    "keywords": ["beginner", "framework", "kit", "laravel", "laravel 4", "starter"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ogün Karakuş",
            "email": "msntx@mail.com",
            "role": "Project Maintainer"
        }
    ],
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php"
        ]
    },
    "require": {
        "cartalyst/sentry": "2.1.*",
        "laravel/framework": "4.1.*",
        "twbs/bootstrap": "3.1.*"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*@dev",
        "orchestra/testbench": "2.1.*@dev",
        "satooshi/php-coveralls": "0.7.*@dev",
        "way/generators": "2.*"
    },
    "scripts": {
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize",
            "php artisan asset:publish --path=\"vendor/twbs/bootstrap/dist/css\" ../assets/css",
            "php artisan asset:publish --path=\"vendor/twbs/bootstrap/dist/js\" ../assets/js",
            "php artisan asset:publish --path=\"vendor/twbs/bootstrap/dist/fonts\" ../assets/fonts"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ]
    },
    "config": {
        "prefered-install": "dist"
    },
    "minimum-stability": "stable"
}
