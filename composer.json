{
    "name": "msntx/kit",
    "description": "A sample application for beginning development with Laravel 4.",
    "keywords": ["beginner", "framework", "kit", "laravel", "laravel 4", "starter"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ogün Karakuş",
            "email": "ogunkarakus@superposta.com"
        }
    ],
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php"
        ]
    },
    "require": {
        "cartalyst/sentry": "2.1.*",
        "laravel/framework": "4.1.*",
        "twbs/bootstrap": "3.1.*"
    },
    "scripts": {
        "pre-update-cmd": ["php artisan clear-compiled", "php artisan optimize"],
        "post-update-cmd": ["php artisan clear-compiled", "php artisan optimize"],
        "post-install-cmd": ["php artisan key:generate"]
    },
    "config": {
        "prefered-install": "dist"
    },
    "minimum-stability": "stable"
}
